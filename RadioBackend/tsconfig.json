{
"compilerOptions": {                       
"target": "es2017",                                
"module": "commonjs",    
"strict": false,                    
"lib": ["es6"],                                  
"allowJs": true,                            
"outDir": "./dist",                              
"rootDir": "./src",                         
"removeComments": true,                     
"typeRoots": [
"./node_modules/@types",
"./src/@types"
],                            
"esModuleInterop": true,                        
"resolveJsonModule": true,
"experimentalDecorators": true,              
"emitDecoratorMetadata": true,               
"skipLibCheck": true,                           
"forceConsistentCasingInFileNames": true ,
"baseUrl": ".",      
"paths": {
"@controllers/*":["./src/controllers/*"],
"@models/*":["./src/models/*"],
"@config/*":["./src/config/*"],
"@errors/*":["./src/errors/*"],
"@middlewares/*":["./src/middlewares/*"],
"@routes/*":["./src/routes/*"],

}
},
"include": [
  "src/**/*"
]
}
